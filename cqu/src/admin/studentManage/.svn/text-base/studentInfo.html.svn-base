			</tr>
			<tr>
				<td class="pro_title">
					
				</td>
				<td class="pro_lab">
					年龄：
				</td>
				<td class="pro_text">
					<input type="text" class="text_input" value="28"/>
				</td>
				
			</tr>
			<tr>
				<td class="pro_title">
					
				</td>
				<td class="pro_lab">
					身份证号：
				</td class="pro_text">
				<td>
					<input type="text" class="text_input" value="610431198605160896"/>
				</td>
				
			</tr>
			<tr>
				<td colspan="4" class="line"> </td>
			</tr>
			<tr>
				<td class="pro_title">
					教育经历：
				</td>
				<td class="pro_lab">
					最高学历：
				</td>
				<td class="pro_text">
					<input type="text" class="text_input" value="本科"/>
